<action>
	<icon>wallpaper</icon>
	<name>Set it as the wallpaper</name>
	<unique-id>1652790135503673-1</unique-id>
	<command>echo %f &gt;&gt; /tmp/wallpaper.txt &amp;&amp; feh --bg-fill &quot;$(tail -n1 /tmp/wallpaper.txt)&quot;</command>
	<description>Sets the selected image as the wallpaper</description>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
